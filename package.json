{
  "name": "RevuTrack",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "dev": "npx concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "npx swc src -w --out-dir dist",
    "watch-dev": "npx nodemon --watch \"dist/**/*\" -e js ./dist/server.js",
    "build": "npx swc src -d dist",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "npx jest"
  },
  "dependencies": {
    "@sentry/node": "^7.61.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^9.0.1",
    "node-cron": "^3.0.2",
    "pg": "^8.11.2",
    "routing-controllers": "^0.10.4",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.8",
    "@types/node-cron": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "typescript": "^5.1.6"
  }
}
